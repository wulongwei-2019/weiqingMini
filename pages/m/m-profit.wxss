.fl{ float: left}
.fr{ float: right}
.oh{ overflow: hidden}
.dn{ display: none}
.mt10{ margin-top: 10rpx }
.mt20{ margin-top: 20rpx}
.mt40{ margin-top: 40rpx}
.brn{ border-right: none}
page{ background-color: #F2F2F2;}
.m-profit .profitNum{ background-color: #fff; padding: 50rpx}
.m-profit .profitNum .title{ font-size: 30rpx; display: block; text-align: center}
.m-profit .profitNum .h-title{ font-size: 40rpx; display: block; text-align: center; color: #FF7433; font-weight: bold; margin-top: 20rpx}
.m-profit .profitList{ margin-top: 20rpx; background-color: #fff}
.m-profit .profitList .list{ padding:25rpx 20rpx;  border-bottom: 1px solid #eee}
.m-profit .profitList .list navigator{overflow: hidden}
.m-profit .profitList .list text{ float: left; font-size: 28rpx}
.m-profit .profitList .list image{ float: right; width: 40rpx; height: 40rpx}
.txBtn{ width: 100%; height: 80rpx;line-height: 80rpx; text-align: center; background-color: #FF7433; color: #fff; font-size: 28rpx; position: fixed; left: 0px; bottom: 0px}







